source 'https://github.com/CocoaPods/Specs.git'

platform :ios, "11.0"

inhibit_all_warnings!
use_frameworks!

def common_pods
    pod 'RxSwift',        '~> 4.0'
    pod 'RxCocoa',        '~> 4.0'
    pod 'KVNProgress',    '~> 2.3.1'
    pod 'Kingfisher',      '~> 4.6.3'
end

target 'Photo Viewer' do
    common_pods
end

target 'Photo ViewerTests' do
    common_pods
end
